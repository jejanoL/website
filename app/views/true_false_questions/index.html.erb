<h1>Listing true_false_questions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @true_false_questions.each do |true_false_question| %>
      <tr>
        <td><%= link_to 'Show', true_false_question %></td>
        <td><%= link_to 'Edit', edit_true_false_question_path(true_false_question) %></td>
        <td><%= link_to 'Destroy', true_false_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New True false question', new_true_false_question_path %>
